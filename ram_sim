#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00A387F8 .scope module, "ram_tb" "ram_tb" 2 3;
 .timescale -9 -12;
v00952B90_0 .var "addr", 3 0;
v00A3D800_0 .var "clk", 0 0;
v00A3D858_0 .var "din", 7 0;
v00A3D8B0_0 .net "dout", 7 0, v00952A88_0; 1 drivers
v00A3D908_0 .var "we", 0 0;
S_00A384C8 .scope module, "uut" "ram" 2 11, 3 1, S_00A387F8;
 .timescale -9 -12;
P_00A32B3C .param/l "ADDR_WIDTH" 3 1, +C4<0100>;
P_00A32B50 .param/l "DATA_WIDTH" 3 1, +C4<01000>;
v00A32BA0_0 .net "addr", 3 0, v00952B90_0; 1 drivers
v00A32BF8_0 .net "clk", 0 0, v00A3D800_0; 1 drivers
v00952A30_0 .net "din", 7 0, v00A3D858_0; 1 drivers
v00952A88_0 .var "dout", 7 0;
v00952AE0 .array "mem", 15 0, 7 0;
v00952B38_0 .net "we", 0 0, v00A3D908_0; 1 drivers
E_00A37418 .event posedge, v00A32BF8_0;
    .scope S_00A384C8;
T_0 ;
    %wait E_00A37418;
    %load/v 8, v00952B38_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v00952A30_0, 8;
    %ix/getv 3, v00A32BA0_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 8, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v00952AE0, 0, 8;
t_0 ;
    %jmp T_0.1;
T_0.0 ;
    %ix/getv 3, v00A32BA0_0;
    %load/av 8, v00952AE0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v00952A88_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00A387F8;
T_1 ;
    %delay 5000, 0;
    %load/v 8, v00A3D800_0, 1;
    %inv 8, 1;
    %set/v v00A3D800_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00A387F8;
T_2 ;
    %vpi_call 2 23 "$dumpfile", "ram_wave.vcd";
    %vpi_call 2 24 "$dumpvars", 1'sb0, S_00A387F8;
    %set/v v00A3D800_0, 0, 1;
    %set/v v00A3D908_0, 0, 1;
    %set/v v00952B90_0, 0, 4;
    %set/v v00A3D858_0, 0, 8;
    %delay 10000, 0;
    %set/v v00A3D908_0, 1, 1;
    %movi 8, 1, 4;
    %set/v v00952B90_0, 8, 4;
    %movi 8, 165, 8;
    %set/v v00A3D858_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 2, 4;
    %set/v v00952B90_0, 8, 4;
    %movi 8, 60, 8;
    %set/v v00A3D858_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 3, 4;
    %set/v v00952B90_0, 8, 4;
    %set/v v00A3D858_0, 1, 8;
    %delay 10000, 0;
    %set/v v00A3D908_0, 0, 1;
    %movi 8, 1, 4;
    %set/v v00952B90_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 2, 4;
    %set/v v00952B90_0, 8, 4;
    %delay 10000, 0;
    %movi 8, 3, 4;
    %set/v v00952B90_0, 8, 4;
    %delay 10000, 0;
    %vpi_call 2 38 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
