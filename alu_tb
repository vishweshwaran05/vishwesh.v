:vpi_module "va_math";
S_011E3C98 .scope module, "alu_tb" "alu_tb" 2 4;
 .timescale -9 -12;
v011ECF98_0 .var "opcode", 2 0;
v011ECFF0_0 .var "operandA", 7 0;
v011EEE30_0 .var "operandB", 7 0;
v011EEE88_0 .net "result", 7 0, v011ECF40_0; 1 drivers
S_011E2B38 .scope module, "uut" "alu" 2 11, 3 1, S_011E3C98;
 .timescale 0 0;
v011E2BC0_0 .net "opcode", 2 0, v011ECF98_0; 1 drivers
v011E2C18_0 .net "operandA", 7 0, v011ECFF0_0; 1 drivers
v011ECEE8_0 .net "operandB", 7 0, v011EEE30_0; 1 drivers
v011ECF40_0 .var "result", 7 0;
E_011EB208 .event edge, v011E2BC0_0, v011E2C18_0, v011ECEE8_0;
    .scope S_011E2B38;
T_0 ;
    %wait E_011EB208;
    %load/v 8, v011E2BC0_0, 3;
    %cmpi/u 8, 0, 3;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 3;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 3;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 3;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 4, 3;
    %jmp/1 T_0.4, 6;
    %cmpi/u 8, 5, 3;
    %jmp/1 T_0.5, 6;
    %cmpi/u 8, 6, 3;
    %jmp/1 T_0.6, 6;
    %cmpi/u 8, 7, 3;
    %jmp/1 T_0.7, 6;
    %set/v v011ECF40_0, 0, 8;
    %jmp T_0.9;
T_0.0 ;
    %load/v 8, v011E2C18_0, 8;
    %load/v 16, v011ECEE8_0, 8;
    %add 8, 16, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.1 ;
    %load/v 8, v011E2C18_0, 8;
    %load/v 16, v011ECEE8_0, 8;
    %sub 8, 16, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.2 ;
    %load/v 8, v011E2C18_0, 8;
    %load/v 16, v011ECEE8_0, 8;
    %and 8, 16, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.3 ;
    %load/v 8, v011E2C18_0, 8;
    %load/v 16, v011ECEE8_0, 8;
    %or 8, 16, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.4 ;
    %load/v 8, v011E2C18_0, 8;
    %load/v 16, v011ECEE8_0, 8;
    %xor 8, 16, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.5 ;
    %load/v 8, v011E2C18_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.6 ;
    %load/v 8, v011E2C18_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 8;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.7 ;
    %load/v 16, v011E2C18_0, 8;
    %load/v 24, v011ECEE8_0, 8;
    %cmp/u 16, 24, 8;
    %mov 16, 4, 1;
    %mov 8, 16, 1;
    %mov 9, 0, 7;
    %set/v v011ECF40_0, 8, 8;
    %jmp T_0.9;
T_0.9 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_011E3C98;
T_1 ;
    %vpi_call 2 19 "$dumpfile", "alu_wave.vcd";
    %vpi_call 2 20 "$dumpvars", 1'sb0, S_011E3C98;
    %vpi_call 2 22 "$monitor", "Time=%0t | opcode=%b | A=%d | B=%d | result=%d", $time, v011ECF98_0, v011ECFF0_0, v011EEE30_0, v011EEE88_0;
    %movi 8, 15, 8;
    %set/v v011ECFF0_0, 8, 8;
    %movi 8, 5, 8;
    %set/v v011EEE30_0, 8, 8;
    %set/v v011ECF98_0, 0, 3;
    %delay 10000, 0;
    %movi 8, 1, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %movi 8, 2, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %movi 8, 3, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %movi 8, 4, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %movi 8, 5, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %movi 8, 6, 3;
    %set/v v011ECF98_0, 8, 3;
    %delay 10000, 0;
    %set/v v011ECF98_0, 1, 3;
    %delay 10000, 0;
